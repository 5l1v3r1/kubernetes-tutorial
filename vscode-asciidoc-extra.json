{
    "Add Tabs": {
        "prefix": "tabs",
        "body": [
            "[tabs]",
            "====",
            "${1:tab1}::",
            "+",
            "--",
            "--",
            "${2:tab2}::",
            "+",
            "--",
            "--",
            "===="
        ],
        "description": "Add Tabs macro"
    },
    "Add clipboard": {
        "prefix": "clipboard",
        "body": [
            "[#${1:clipboardid}]",
            "[source,${2:bash},subs=\"+macros,+attributes\"]",
            "----",
            "${3}",
            "----",
            "copyToClipboard::$1[]"
        ],
        "description": "Add Source with Clipboard"
    }
}